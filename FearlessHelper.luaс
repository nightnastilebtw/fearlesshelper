                        imgui.SameLine()
                        imgui.InputText(u8'Отправка в /ad', nabortext3) 
                        imgui.NullText()
                        imgui.SameLine()
                        radiostation()
                        imgui.NullText()
                        imgui.SameLine()
                        imgui.SliderInt(u8"КД отправки", nabint, 1, 300, 1.0)
                        imgui.SameLine()
                        imgui.TextQuestion(u8'Ставьте не менее 60, что-бы не получить мут от бота. Значения устанавливаются в секундах!')
                        imgui.NullText()
                        imgui.SameLine()
                        if imgui.Button(u8'Активация/Деактивация', actnabor) then
                            naborspam()
                        end                   
                    end
                    imgui.TextQuestion(u8'Флудер семьи. /fspam - активация. Пишет ваш текст в чат семьи. Устанавливайте кд в СЕКУНДАХ')
                    imgui.SameLine()
                    imgui.Checkbox(u8'Флудер семьи.', famflood)
                    if famflood.v then
                        
                        
                        imgui.TextQuestion(u8'КД между вашими сообщениями в секундах')
                        imgui.SameLine()
                        imgui.SliderInt(u8"КД между срабатыванием", famint, 1, 180, 1.0)
                        imgui.NullText()
                        imgui.SameLine()
                        imgui.InputText(u8'Пишет данный текст в чат вашей фамы', famtext3)
                        imgui.NullText()
                        imgui.SameLine()
                        if imgui.Button(u8'Активация/Деактивация##8515', actfam) then
                            famchatflooder()
                        end
                    end
                    
