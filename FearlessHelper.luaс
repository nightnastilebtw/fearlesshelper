                        imgui.TextQuestion(u8'Устанавливает задержку между срабатываением флудера')
                        imgui.SameLine()
                        imgui.SliderInt(u8"КД между работой флудера", orgint2, 1, 300, 1.0)
                        imgui.NullText()
                        imgui.SameLine()
                        if imgui.Button(u8'Активация/Деактивация##51', actflood) then
                            prospam()
                        end
                    end
                    
                    imgui.TextQuestion(u8'Флудер о наборе. Пишет ваш текст каждые N-ое кол-во секунд которые вы поставили. Если не хотите что-бы он отправлялся куда-то, оставьте поле пустым')
                    imgui.SameLine()
                    imgui.Checkbox(u8'Флудер набора', nabor)
                    if nabor.v then                       
                        imgui.NullText()
                        imgui.SameLine()
