function drmap()
	state = not state
	sampAddChatMessage(state and tag..'Карта закладок включена!' or tag..'Карта закладок выключена!', -1)
	if not state then remove_all() end
end



function getCurrentServer(name)
	if name:find('Advance RolePlay') then return 1
	elseif name:find('Samp%-Rp') then return 2
	elseif name:find('Pears Project') then return 3
	elseif name:find('Arizona') then return 4 end
end



