local status = inicfg.load(mainIni, 'fearlesshelper.ini')
if not doesFileExist('moonloader/config/fearlesshelper.ini') then inicfg.save(mainIni, 'fearlesshelper.ini') end


local functionslist = [[
    Функции Fearless Helper:
    /sc - Быстрый спавн транспорта банды
    /fc - Быстрая заправка транспорта банды
    /sk - Открытие/закрытие склада банды
    /spamm - Запустить флуд в чат банды
    /fspam - Запустить флуд в чат фамы
    /fm [ID] - Быстрый мут
    /fu [ID] - Быстрое увольнение
    /gs [ID] [SkinID] - Быстрая выдача скина
    /gr [ID] [Rank] - Быстрая выдача ранга
    /cb - Узнать онлайн всех банд и мафий
    /bcl - Очистить буфер памяти
    /cde [amount] - Сделать дигла [Из Материалов]
    /cm4 [amount] - Сделать эмки  [Из Материалов]
    /de [amount] - Взять Deagle из Инвентаря
    /m4 [amount] - Взять M4 из Инвентаря
    /uz [amount] - Взять Uzi из Инвентаря
    /rfl [amount] - Взять Country Rifle из Инвентаря
    /ak [amount] - Взять AK-47 из Инвентаря
    /shg [amount] - Взять Shotgun из Инвентаря
    
    Зажимаете shift + w - Ускоряетесь на мото или вело
    За основу был взят Ghetto Helper]]
----------------------------------------------------------------------
--пїЅпїЅпїЅпїЅпїЅпїЅпїЅпїЅпїЅпїЅпїЅпїЅпїЅпїЅпїЅ пїЅпїЅпїЅпїЅпїЅпїЅ пїЅпїЅпїЅпїЅпїЅ
----------------------------------------------------------------------
function main()
    while not isSampAvailable() do wait(200) end
    autoupdate("https://raw.githubusercontent.com/nightnastilebtw/fearlesshelper/main/version.json", '['..string.upper(thisScript().name)..']: ', "https://vk.com/fearlessqd")
    local font = renderCreateFont("Arial", 10, 5)
	sampAddChatMessage(tag..'Успешно загружен! Настройка: {9461FF}/fh(elper) или же с помощью клавиши F3!', -1)
    sampRegisterChatCommand('fh', function() window.v = not window.v end)
	sampRegisterChatCommand('fhelper', function() window.v = not window.v end)
    sampRegisterChatCommand('cb', function()
		local text = texts[current_server]
		for i = 1, #clists[current_server] do
			local online = 0
			for l = 0, 1004 do
				if sampIsPlayerConnected(l) then
					if sampGetPlayerColor(l) == clists[current_server][i] then online = online + 1 end
				end
			end
			text = text:gsub('$CLR', ('%06X'):format(bit.band(clists[current_server][i], 0xFFFFFF)), 1)
			text = text:gsub('$CNT', online, 1)
		end
		for w in text:gmatch('[^\r\n]+') do sampAddChatMessage(w, -1)end
	end)
    
    sampRegisterChatCommand('sc', sc)
    sampRegisterChatCommand('fc', fc)
	sampRegisterChatCommand('sk', sk)
	sampRegisterChatCommand("fm", fm)
    sampRegisterChatCommand('drmap', drmap)
    sampRegisterChatCommand("fu", fu)
    sampRegisterChatCommand("gs", skins)
    sampRegisterChatCommand('gr', rank)
    sampRegisterChatCommand('asklad', avtosklad)
    sampRegisterChatCommand('vra', function(text) 
        adsend = true
        sampSendChat('/vr '..text)
    end)
    sampRegisterChatCommand('vr', function(text)
        adsend = false
        sampSendChat('/vr '..text)
    end)
	if sampGetGamestate() == 3 then
        current_server = getCurrentServer(sampGetCurrentServerName())
    end
    sampRegisterChatCommand('de',function(arg) getguninvent('de',arg) end)
    image = imgui.CreateTextureFromFile("moonloader/sczones/groove.jpg")
    image1 = imgui.CreateTextureFromFile("moonloader/sczones/aztec.jpg")
    imagerifa = imgui.CreateTextureFromFile("moonloader/sczones/rifa.jpg")
    imageballas = imgui.CreateTextureFromFile("moonloader/sczones/ballas.jpg")
    imagevagos = imgui.CreateTextureFromFile("moonloader/sczones/vagos.jpg")
    imagenw = imgui.CreateTextureFromFile("moonloader/sczones/nw.jpg")
    imageskport = imgui.CreateTextureFromFile("moonloader/sczones/portzone.jpg")
    priton1 = imgui.CreateTextureFromFile("moonloader/sczones/priton1.jpg")
    priton2 = imgui.CreateTextureFromFile("moonloader/sczones/priton2.jpg")
    priton3 = imgui.CreateTextureFromFile("moonloader/sczones/priton3.jpg")
    priton4 = imgui.CreateTextureFromFile("moonloader/sczones/priton4.jpg")
    priton5 = imgui.CreateTextureFromFile("moonloader/sczones/priton5.jpg")
    priton6 = imgui.CreateTextureFromFile("moonloader/sczones/priton6.jpg")
    priton7 = imgui.CreateTextureFromFile("moonloader/sczones/priton7.jpg")
    priton8 = imgui.CreateTextureFromFile("moonloader/sczones/priton8.jpg")
    sampRegisterChatCommand('m4',function(arg) getguninvent('m4',arg) end)
    sampRegisterChatCommand('uz',function(arg) getguninvent('uz',arg) end)
    sampRegisterChatCommand('ak',function(arg) getguninvent('ak',arg) end)
    sampRegisterChatCommand('mp5',function(arg) getguninvent('mp5',arg) end)
    sampRegisterChatCommand('shg',function(arg) getguninvent('shg',arg) end)
    sampRegisterChatCommand('rfl',function(arg) getguninvent('rfl',arg) end)
    sampRegisterChatCommand('probiv', cmd_probiv)
    sampRegisterChatCommand("bcl", _con)
    sampRegisterChatCommand("cde", function(arg)
        if mainIni.settings.CmdDe then
            lua_thread.create(function()
                if arg == '' or arg == nil or arg == 0 then
                    sampAddChatMessage(tag..'Введите кол-во патрон', -1)
                else
                    ptde = arg
                    sampSendChat('/creategun')
                    wait(100)
                    sampSendDialogResponse(7546, 1, 0, _)
                    wait(100)
                    sampSetCurrentDialogEditboxText(ptde)
                    wait(100)
                    sampCloseCurrentDialogWithButton(1)
                end
            end)
        else 
            sampSendChat('/1')
        end
     end)
     sampRegisterChatCommand("cm4", function(arg)
        if mainIni.settings.CmdM4 then
            lua_thread.create(function()
                if arg == '' or arg == nil or arg == 0 then
                    sampAddChatMessage(tag..'Введите кол-во патрон', -1)
                else
                    ptm4 = arg
                    sampSendChat('/creategun')
                    wait(100)
                    sampSendDialogResponse(7546, 1, 3, _)
                    wait(100)
                    sampSetCurrentDialogEditboxText(ptm4)
                    wait(100)
                    sampCloseCurrentDialogWithButton(1)
                end
            end)
        else 
            sampSendChat('/1')
        end
    end)
   

----------------------------------------------------------------------
--РЕГИСТРИРОВАНИЕ КОМАНД СВЕРХУ
----------------------------------------------------------------------


    while true do --пїЅпїЅпїЅпїЅ пїЅпїЅпїЅпїЅ
        wait(0)
        if isKeyJustPressed(0x72) and not sampIsChatInputActive() and not sampIsDialogActive() then
            window.v = not window.v
            
        end
        
        if isKeyDown(0x12) and isKeyJustPressed(0x31) and bind1.v and not sampIsChatInputActive() and not sampIsDialogActive() then
            sampSendChat("/usedrugs 3")
            wait(100)
            sampSendChat(sbivtext.v)
        
        end
        if isKeyDown(0x12) and isKeyJustPressed(0x32) and bind2.v and not sampIsChatInputActive() and not sampIsDialogActive() then
            sampSendChat("/usedrugs 3")
            
        
        end
        if isKeyDown(0x12) and isKeyJustPressed(0x33) and bind3.v and not sampIsChatInputActive() and not sampIsDialogActive() then
            sampSendChat("/armour")
            
        
            
        end
        if isKeyDown(0x12) and isKeyJustPressed(0x34) and bind4.v and not sampIsChatInputActive() and not sampIsDialogActive() then
            sampSendChat("/mask")
            
        end
        if isKeyJustPressed(0x52) and not sampIsChatInputActive() and bind9.v and not sampIsDialogActive() then
            sampSendChat("q") 
        
        end
        if isKeyDown(0x12) and isKeyJustPressed(0x35) and bind5.v and not sampIsChatInputActive() and not sampIsDialogActive() then
            sampProcessChatInput("/de "..deaglepatrons.v)
       
        end
        if isKeyDown(0x12) and isKeyJustPressed(0x36) and bind6.v and not sampIsChatInputActive() and not sampIsDialogActive() then
            sampProcessChatInput("/m4 "..m4patrons.v)
            
        end
        if isKeyJustPressed(0x4C) and bind8.v and not sampIsChatInputActive() and not sampIsDialogActive() then
            sampSendChat("/lock")
            
        end
       
        if isKeyDown(0x12) and isKeyJustPressed(0x30) and bind7.v and not sampIsChatInputActive() and not sampIsDialogActive() then
            sampSendChat("/point")
            
        end

        if isKeyDown(0x12) and isKeyJustPressed(0x51) and bind10.v and not sampIsChatInputActive() and not sampIsDialogActive() then
            sampSendChat("/anim 9")
            
        end
        if skladsocr.v and isKeyJustPressed(0x31) and not sampIsChatInputActive() and not sampIsDialogActive() then
            wait(100)
            sampProcessChatInput("/sk")
           
        end
        if skladsocr.v and isKeyJustPressed(0x32) and not sampIsChatInputActive() and not sampIsDialogActive() then
            wait(100)
            sampProcessChatInput("/sc")
        end
        if skladsocr.v and isKeyJustPressed(0x33) and not sampIsChatInputActive() and not sampIsDialogActive() then
            wait(100)
            sampProcessChatInput("/fc")
        end
        if car1.v and isKeyJustPressed(0x30) then
            wait(0)
            sampSendChat("/cars")
        end
  
        if isCharOnAnyBike(playerPed) and isKeyCheckAvailable() and isKeyDown(0xA0) then	-- onBike&onMoto SpeedUP [[LSHIFT]] --
			if bike[getCarModel(storeCarCharIsInNoSave(playerPed))] then
				setGameKeyState(16, 255)
				wait(10)
				setGameKeyState(16, 0)
			elseif moto[getCarModel(storeCarCharIsInNoSave(playerPed))] then
				setGameKeyState(1, -128)
				wait(10)
				setGameKeyState(1, 0)
			end
		end
        if faminvite.v then
            local result, target = getCharPlayerIsTargeting(playerHandle)
            if result then result, playerid = sampGetPlayerIdByCharHandle(target) end 
            if result and isKeyJustPressed(0x58) then 
                name = sampGetPlayerNickname(playerid) 
                wait(10)
                sampSendChat('/faminvite '..playerid)
                    wait(3000)
                    if inv_famrank.v ~= 1 and inv_famrank.v ~= nil then
                    sampSendChat('/setfrank '..playerid..' '..inv_famrank.v)
                    end
                    wait(250)
                    sampAddChatMessage(tag..'Вы приняли игрока в семью с ником: '..name..' | Ранг: '..inv_famrank.v, -1)
                    
                
            end
        end
        
        
		
		if isCharOnFoot(playerPed) and isKeyDown(0x31) and isKeyCheckAvailable() then -- onFoot&inWater SpeedUP [[1]] --
			setGameKeyState(16, 256)
			wait(10)
			setGameKeyState(16, 0)
		elseif isCharInWater(playerPed) and isKeyDown(0x31) and isKeyCheckAvailable() then
			setGameKeyState(16, 256)
			wait(10)
			setGameKeyState(16, 0)
		end
        if state then
			local A = { getCharCoordinates(PLAYER_PED) }
			for i, B in ipairs(coords) do
				local dist = getDistanceBetweenCoords3d(A[1], A[2], A[3], B[1], B[2], B[3])
				if dist <= radius and pool[i] == nil then
					pool[i] = addBlipForCoord(B[1], B[2], B[3])
					changeBlipColour(pool[i], color)
				elseif dist > radius and pool[i] ~= nil then
					removeBlip(pool[i])
					pool[i] = nil
				end
			end
		end
    
        if rp_invite.v then
            local result, target = getCharPlayerIsTargeting(playerHandle)
            if result then result, playerid = sampGetPlayerIdByCharHandle(target) end 
            if result and isKeyJustPressed(0x5A)  then 
                _, id = sampGetPlayerIdByCharHandle(target)
			    if sampIsPlayerConnected(id) then
				lvl, name = sampGetPlayerScore(id), sampGetPlayerNickname(id)
				if tonumber(lvl) >= tonumber(mainIni.settings.minlvl) then 
                    sampSendChat('/me передал бандану')
                    wait(1000)
                    sampSendChat('/invite '..playerid)
                    wait(250)
                    sampAddChatMessage(tag..'Вы приняли игрока с ником: '..name..' | Ранг:  '..inv_ranks.v, -1)
                else
                    wait(100)
                    sampAddChatMessage(tag..'У игрока уровень недостаточно высок!', -1)
                end
            end
            end
        end

        


        imgui.Process = window.v
    end
    end
    
                
                
            
              
  
function encda()
    commandslist.v = u8:encode(functionslist)
end

commandslist = imgui.ImBuffer(65536)



function skins(param)
	if state then
		state = false
	elseif not param:match('%d+') then
		sampAddChatMessage(tag..'Правильный ввод: /gs [ID] [SkinID]', -1)
	else
			id = tonumber(param)
			state = true
			sampSendChat('/giveskin '..param..' '..param..'')
            sampSendDialogResponse(9188, 1, 0, -1)
            sampCloseCurrentDialogWithButton(0)
			state = false
	end
end

function rank(param)
	if state then
		state = false
	elseif not param:match('%d+') then
		sampAddChatMessage(tag..'Правильный ввод: /gr [ID] [Rank]', -1)
	else
			id = tonumber(param)
			state = true
			sampSendChat('/giverank '..param..' '..param..'')
			state = false
	end
end

function fm(param)
    if fastmute.v then
	if state then
		state = false
	elseif not param:match('%d+') then
		sampAddChatMessage(tag..'Правильный ввод: /fm [ID]', -1)
	else
			id = tonumber(param)
			state = true
			sampSendChat('/fmute '..param..' '..muteminutes.v..' '..u8:decode(mutetext.v), -1)
			state = false
    end
	end
end

function fu(param)
	if state then
		state = false
	elseif not param:match('%d+') then
		sampAddChatMessage(tag..'Правильный ввод: /fu [ID]', -1)
	else
			id = tonumber(param)
			state = true
			sampSendChat('/uninvite '..param..' '..u8:decode(uvaltext.v), -1)
			state = false
	end
end


function nameTagOn()
    local pStSet = sampGetServerSettingsPtr();
    NTdist = mem.getfloat(pStSet + 39)
    NTwalls = mem.getint8(pStSet + 47)
    NTshow = mem.getint8(pStSet + 56)
    mem.setfloat(pStSet + 39, 1488.0)
    mem.setint8(pStSet + 47, 0)
    mem.setint8(pStSet + 56, 1)
    nameTag = true
end
  
function nameTagOff()
    local pStSet = sampGetServerSettingsPtr();
    mem.setfloat(pStSet + 39, NTdist)
    mem.setint8(pStSet + 47, NTwalls)
    mem.setint8(pStSet + 56, NTshow)
    nameTag = false
end




function imgui.OnDrawFrame()
    if window.v then
        imgui.SetNextWindowPos(imgui.ImVec2(sw / 2, sh / 2), imgui.Cond.FirstUseEver, imgui.ImVec2(0.5, 0.5))
