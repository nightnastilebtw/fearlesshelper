function radiostation()
    imgui.Text(u8'Выберите радиостанцию')
    imgui.NullText()
    imgui.SameLine()
    imgui.Checkbox(u8'LS', radiols)
    
    
    imgui.SameLine()
    imgui.Checkbox(u8'LV', radiolv)

    
    imgui.SameLine()
    imgui.Checkbox(u8'SF', radiosf)
    imgui.NullText()
    imgui.SameLine()
    imgui.Text(u8'Скрытие окошка отмены если у вас уже зарегистрировано сообщение')
    imgui.SameLine()
    imgui.Checkbox(u8'##87151n', otmena)


    
end
    

